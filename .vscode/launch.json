{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Python: Remote Attach (Docker)",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/src/backend",
          "remoteRoot": "/app"
        }
      ],
      "justMyCode": false,
      "showReturnValue": true
    }
  ]
}
